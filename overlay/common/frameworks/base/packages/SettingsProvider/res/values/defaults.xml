<resources>
    <string name="def_backup_transport">com.google.android.backup/.BackupTransportService</string>

    <!-- Enable notification counters in statusbar -->
    <integer name="def_notif_count">1</integer>

    <!-- Right QS pulldown -->
    <integer name="def_qs_quick_pulldown">1</integer>

    <!-- Disable annoying sound notifications -->
    <bool name="def_dtmf_tones_enabled">false</bool>
    <integer name="def_lockscreen_sounds_enabled">0</integer>
    <bool name="def_sound_effects_enabled">false</bool>

    <!-- Set battery style to circle with percentage text -->
    <integer name="def_battery_style">2</integer>
    <integer name="def_show_battery_percent">1</integer>

    <!-- Enable andvanced reboot menu -->
    <bool name="def_advanced_reboot">true</bool>

    <!-- Enable privacy guard for new apps -->
    <bool name="def_privacy_guard_default">true</bool>

    <!-- Disable wifi notification -->
    <bool name="def_networks_available_notification_on">false</bool>
</resources>
